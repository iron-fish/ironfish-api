{
  "name": "ironfish-api",
  "version": "0.0.0",
  "description": "HTTP server to interface with Iron Fish accounts",
  "private": true,
  "author": "Iron Fish <contact@ironfish.network> (https://ironfish.network)",
  "repository": "iron-fish/ironfish",
  "license": "MPL-2.0",
  "homepage": "https://github.com/iron-fish/ironfish-api",
  "engines": {
    "node": "16.x"
  },
  "keywords": [
    "api"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf ./build",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prestart": "bash bin/check_for_dotenv.sh",
    "start": "node build/main.js",
    "start:worker": "node build/worker.js",
    "test": "jest --runInBand",
    "test:clean": "prisma migrate reset --force --skip-generate > /dev/null && jest --runInBand",
    "db:client:generate": "prisma generate",
    "db:migrate": "prisma migrate dev"
  },
  "devDependencies": {
    "@nestjs/testing": "8.2.6",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/faker": "5.5.9",
    "@types/jest": "27.4.0",
    "@types/passport": "1.0.7",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "dotenv": "16.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "faker": "5.5.3",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "prisma": "3.9.1",
    "supertest": "6.2.2",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5",
    "uuid": "8.3.2"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "1.22.0",
    "@magic-sdk/admin": "1.3.4",
    "@nestjs/common": "8.2.6",
    "@nestjs/config": "1.1.6",
    "@nestjs/core": "8.2.6",
    "@nestjs/microservices": "8.2.6",
    "@nestjs/passport": "8.1.0",
    "@nestjs/platform-express": "8.2.6",
    "@nestjs/swagger": "5.2.0",
    "@prisma/client": "3.8.1",
    "@sindresorhus/is": "4.4.0",
    "assert": "2.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cluster": "0.7.7",
    "compression": "1.7.4",
    "dd-trace": "2.0.0",
    "express": "4.17.2",
    "graphile-worker": "0.12.2",
    "helmet": "5.0.2",
    "hot-shots": "9.0.0",
    "joi": "17.6.0",
    "passport": "0.5.2",
    "pino": "7.6.5",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.2",
    "swagger-ui-express": "4.3.0",
    "type-fest": "2.11.1",
    "ulid": "2.3.0"
  }
}
