{
  "name": "ironfish-api",
  "version": "0.0.0",
  "description": "HTTP server to interface with Iron Fish accounts",
  "private": true,
  "author": "Iron Fish <contact@ironfish.network> (https://ironfish.network)",
  "repository": "iron-fish/ironfish",
  "license": "MPL-2.0",
  "homepage": "https://github.com/iron-fish/ironfish-api",
  "engines": {
    "node": "16.x"
  },
  "keywords": [
    "api"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf ./build",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prestart": "bash bin/check_for_dotenv.sh",
    "start": "node build/main.js",
    "start:dev": "webpack --config webpack.config.js --watch",
    "start:worker": "node build/worker.js",
    "test:keepdb": "dotenv -e .env.test -- jest --runInBand",
    "test": "dotenv -e .env.test -- prisma migrate reset --force --skip-generate > /dev/null && yarn test:keepdb",
    "db:client:generate": "prisma generate",
    "db:client:format": "prisma format",
    "db:migrate": "prisma migrate dev"
  },
  "devDependencies": {
    "@nestjs/testing": "8.4.7",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.14",
    "@types/faker": "5.5.9",
    "@types/jest": "27.5.0",
    "@types/passport": "1.0.11",
    "@types/semver": "7.3.12",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "dotenv": "16.0.3",
    "dotenv-cli": "6.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-jest": "27.1.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "faker": "5.5.3",
    "jest": "27.5.1",
    "prettier": "2.7.1",
    "prisma": "4.5.0",
    "run-script-webpack-plugin": "0.1.1",
    "supertest": "6.3.0",
    "ts-jest": "27.1.4",
    "ts-loader": "9.4.1",
    "typescript": "4.7.4",
    "uuid": "9.0.0",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "1.31.0",
    "@magic-sdk/admin": "1.4.1",
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "8.4.7",
    "@nestjs/microservices": "8.4.7",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/swagger": "5.2.1",
    "@prisma/client": "4.5.0",
    "@sindresorhus/is": "4.6.0",
    "assert": "2.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cluster": "0.7.7",
    "compression": "1.7.4",
    "dd-trace": "3.5.0",
    "express": "4.18.2",
    "graphile-worker": "0.13.0",
    "helmet": "6.0.0",
    "hot-shots": "9.3.0",
    "joi": "17.6.4",
    "passport": "0.6.0",
    "pino": "8.7.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.7",
    "semver": "7.3.8",
    "swagger-ui-express": "4.5.0",
    "type-fest": "2.19.0",
    "ulid": "2.3.0"
  }
}
